{
  "name": "vue-ellipsis-text",
  "description": "A vue component for web developers",
  "version": "1.0.3",
  "author": "BabyChin",
  "license": "MIT",
  "private": false,
  "keywords": [
    "ellipsisText"
  ],
  "main": "dist/ellipsisText.umd.js",
  "browser": {
    "tipso": "src/ellipsisText/index.js"
  },
  "_requested": {
    "type": "tag",
    "registry": true,
    "raw": "ellipsisText",
    "name": "ellipsisText",
    "escapedName": "ellipsisText",
    "rawSpec": "",
    "saveSpec": null,
    "fetchSpec": "latest"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "start": "npm run dev",
    "test": "jest /test/unit/ellipsisText.spec.js --detectOpenHandles",
    "build:demo": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build:amd": "webpack --config config/webpack.config.amd.js --hide-modules",
    "build:cmd": "webpack --config config/webpack.config.cmd.js --hide-modules",
    "build:umd": "webpack --config config/webpack.config.umd.js --hide-modules",
    "build": "npm run build:amd && npm run build:cmd && npm run build:umd"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "autoprefixer": "^9.2.1",
    "vue-jest": "^3.0.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.6.11",
    "jest": "^23.6.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}